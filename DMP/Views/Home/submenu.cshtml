@{
    Layout = "~/Views/Shared/_genLayout.cshtml";
}
@section AddToHead{
    <style>
        .custom-heading-div {
            color: #fff;
            font-size: 24px;
        }

        .accordion-group {
            border: 0px solid #fff;
            margin-bottom: 25px;
        }

        .btn-default.btn-outline {
            color: darkorange;
            background-color: transparent;
            border-color: darkorange;
        }

            .btn-default.btn-outline:hover {
                color: white;
                /*background-color: darkorange;*/
                border-color: darkorange;
            }

        .pnl-height {
            height: 400px;
        }

        .dropdown-item {
            padding-left: 20%;
            text-decoration: underline;
        }

        .dropdown {
            margin-right: 15px;
        }
    </style>
}
