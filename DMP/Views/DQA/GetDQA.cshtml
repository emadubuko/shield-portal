
@{
    ViewBag.Title = "GetDQA";
}

@section AddToHead{
    @Styles.Render("~/bundles/CreateDMPCss")
    <style>
        .panel-warning {
            border-color: #929b17;
        }

            .panel-warning > .panel-heading {
                color: #FFF;
                background-color: #929b17;
                border-color: #929b17;
            }

            
        .table > thead > tr > th {
             font-weight:bold
        }
        .abbrev-full{
            background-color:lightblue
        }
    </style>
}

<div class="main-content">

    <div class="row">

        <div class="col-lg-12">
            <div class="panel panel-default">
                <div class="panel-heading clearfix" style="padding:15px">
                    <h4 class="panel-title">SITE DQA REPORT</h4>
                    <input type="hidden" id="metadataId" value="@ViewBag.metadataId" />
                </div>
                <div class="panel-body">

                  

                    <div class="tabs-container">
                        <ul class="nav nav-tabs">
                            <li class="active"><a aria-expanded="true" href="#home" data-toggle="tab">DQA Summary Result</a></li>
                            <li><a aria-expanded="false" href="#profile" data-toggle="tab">DQA</a></li>
                        </ul>
                        <div class="tab-content">
                            <div class="tab-pane active" id="home">
                                <div class="panel-body">
                                    <table class="table table-bordered table-condensed table-hover">
                                        <tr>
                                            <td><strong>LEGEND : </strong></td>
                                            <td class="abbrev">COM</td>
                                            <td class="abbrev-full">COMPLETENESS</td>
                                            <td class="abbrev">CON</td>
                                            <td class="abbrev-full">CONSISTENCY</td>
                                            <td class="abbrev">PRE</td>
                                            <td class="abbrev-full">PRECISION</td>
                                            <td class="abbrev">INT</td>
                                            <td class="abbrev-full">INTEGRITY</td>
                                            <td class="abbrev">VAL</td>
                                            <td class="abbrev-full">VALIDITY</td>


                                        </tr>
                                    </table>
                                    <div class="table-responsive" id="divTable1">

                                    </div>

                                </div>
                            </div>
                            <div class="tab-pane" id="profile">
                                <div class="panel-body">
                                    <div class="table-responsive" id="divTable">

                                    </div>

                                </div>
                            </div>
                        </div>
                    </div>



                    

                </div>
            </div>
        </div>

    </div>

</div>



@section Scripts
{
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/dataTables")
    @Scripts.Render("~/bundles/loader")
    <script src="~/Scripts/fns/base.js" type="text/javascript"></script>
    <script src="~/Scripts/fns/fn_dqa.js" type="text/javascript"></script>
   <script src="~/Scripts/fns/jsonTable.js" type="text/javascript"></script>

    <script>


        $(document).ready(function () {
            getDQA($("#metadataId").val());

        });
    </script>
}




