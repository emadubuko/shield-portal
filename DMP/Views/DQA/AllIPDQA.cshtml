
@{
    ViewBag.Title = "IpDQA";
}

@section AddToHead{
    @Styles.Render("~/bundles/CreateDMPCss")
    <style>
        .panel-warning {
            border-color: #929b17;
        }

            .panel-warning > .panel-heading {
                color: #FFF;
                background-color: #929b17;
                border-color: #929b17;
            }

        .table > thead > tr > th {
            font-weight: bold;
        }

        .abbrev-full {
            background-color: lightblue;
        }
    </style>
}

<div class="main-content">

    <div class="row">

        <div class="col-lg-12">



            <div class="row">
                <div class="col-lg-12">
                    <div class="panel panel-default">
                        <div class="panel-heading clearfix" style="padding:15px">
                            <h4 class="panel-title">DQA Summary Result</h4>
                            <a style="float:right" id="downloadbtn" class="btn btn-default btn-sm btn-outline">
                                <i class="icon-download"></i><span>Download DQA Dimensions</span>
                            </a>
                        </div>
                        <div class="panel-body">
                            <table class="table table-bordered table-condensed table-hover">
                                <tr>
                                    <td><strong>LEGEND : </strong></td>
                                    <td class="abbrev">COM</td>
                                    <td class="abbrev-full">COMPLETENESS</td>
                                    <td class="abbrev">CON</td>
                                    <td class="abbrev-full">CONSISTENCY</td>
                                    <td class="abbrev">PRE</td>
                                    <td class="abbrev-full">PRECISION</td>
                                    <td class="abbrev">INT</td>
                                    <td class="abbrev-full">INTEGRITY</td>
                                    <td class="abbrev">VAL</td>
                                    <td class="abbrev-full">VALIDITY</td>


                                </tr>
                            </table>
                            <div id="divTable" class="table-responsive"></div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-12">
                    <div class="panel panel-default">
                        <div class="panel-heading clearfix" style="padding:15px">
                            <h4 class="panel-title">IP Listing</h4>

                        </div>
                        <div class="panel-body">
                            <div class="table-responsive">
                                <table class="table table-bordered table-striped table-hover" id="output">
                                    <thead>
                                        <tr>

                                            <th>IP Name</th>
                                            <th>ShortName</th>
                                            <th>Address</th>
                                            <th>Phone Number</th>

                                        </tr>
                                    </thead>
                                    <tbody></tbody>
                                </table>
                            </div>
                        </div>
                    </div>

                </div>
            </div>


        </div>

    </div>

</div>



@section Scripts
{
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/dataTables")
    @Scripts.Render("~/bundles/loader")
    <script src="~/Scripts/jsonTable.js"></script>
    <script src="~/Scripts/fns/base.js" type="text/javascript"></script>
    <script src="~/Scripts/fns/fn_dqa.js" type="text/javascript"></script>

    <script>
        $(document).ready(function () {
            $(".page-container").attr('class', 'page-container sidebar-collapsed');

            $("#downloadbtn").click(function (e, n) {
                $.ajax({
                    type: "POST",
                    url: "/api/DQAApi/GenerateDQADimensions",
                    contentType: "application/json; charset=utf-8",
                    cache: false,
                }).done(function () {
                    window.open('/Report/Downloads/DQADimensions.csv', '_blank', '');
                    // window.location = '/Report/Downloads/DQADimensions.csv';///Downloads/' + filename;
                }).error(function (xhr, status, err) {
                    alert(err);
                    console.log(err);
                });
                return false;
            });

            $(document).ready(function () {
                getAllIPDQASummary();

            });
        })        
    </script>
}
