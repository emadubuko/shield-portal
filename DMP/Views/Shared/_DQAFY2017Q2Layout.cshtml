@{
    Layout = "~/Views/Shared/_genLayout.cshtml";
}
@section AddToHead{
@if (IsSectionDefined("AddToHead"))
{
    @RenderSection("AddToHead", required: false)
}
}
<div class="main-content" style="padding-top: 0px;">
    <div class="row" id="breadcrumb">
        <div class="col-sm-5 col-sm-push-3">
            <ol class="breadcrumb breadcrumb-2">
                <li><a href="/"><i class="icon-folder"></i>Home</a></li>
                <li><a href="/Home/submenu?id=dmp">Data Management</a></li>
                <li class="active">DQA FY2017 Q3</li>
            </ol>
        </div>
    </div>

    <div class="row">
        @*side bar*@
        <div class="col-md-3">
            <div class="page-sidebar">
                <div class="sidebar"> 
                    <ul id="side-nav" class="main-menu navbar-collapse collapse">
                        <li>
                            <a href=@Url.Action("index", "DQAFY2017Q2") style="color:#029b17;">
                                <i class="fa fa-home"></i><span class="title">&nbsp; Home</span>
                            </a>
                        </li>
                        <li>
                            <a href=@Url.Action("Analytics", "DQAFY2017Q2") style="color:#029b17;">
                                <i class="fa fa-bar-chart"></i><span class="title">&nbsp; Dashboard</span>
                            </a>
                        </li>
                        <li>
                            <a href=@Url.Action("IpDQA", "DQAFY2017Q2") style="color:#029b17;">
                                <i class="fa fa-file-text-o"></i><span class="title">&nbsp; DQA Report</span>
                            </a>
                        </li>
                        <li>
                            <a href=@Url.Action("DQAResult", "DQAFY2017Q2") style="color:#029b17;">
                                <i class="fa fa-newspaper-o"></i><span class="title">&nbsp; DQA Result</span>
                            </a>
                        </li>
                        <li>
                            <a href=@Url.Action("UploadDQA", "DQAFY2017Q2") style="color:#029b17;">
                                <i class="fa fa-cloud-upload"></i><span class="title">&nbsp; Upload DQA</span>
                            </a>
                        </li>
                        @*<li>
                            <a href=@Url.Action("UploadRadet", "DQAFY2017Q2") style="color:#029b17;">
                                <i class="fa fa-cloud-upload"></i><span class="title">&nbsp; Upload RADET</span>
                            </a>
                        </li>*@
                        <li>
                            <a href=@Url.Action("UploadPivotTable", "DQAFY2017Q2") style="color:#029b17;">
                                <i class="fa fa-cloud-upload"></i><span class="title">&nbsp; Site selection <br />&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;(Pivot Table)</span>
                            </a>
                        </li>
                        <li class="navigation-header">Downloads</li>
                        <li>
                            <a href="~/DQAFY2017Q2/DownloadDQATool" style="color:deeppink;">
                                <i class="fa fa-download"></i><span class="title">&nbsp; DQA tool</span>
                            </a>
                        </li>
                        <li>
                            <a href="~/DQAFY2017Q2/downloadDQAResource?fileType=PivotTable" style="color:deeppink;">
                                <i class="fa fa-download"></i><span class="title">&nbsp; Pivot Table Template</span>
                            </a>
                        </li>
                        <li>
                            <a href="~/DQAFY2017Q2/downloadDQAResource?fileType=DQAUserGuide" style="color:deeppink;">
                                <i class="fa fa-download"></i> <span class="title">&nbsp; DQA user guide</span>
                            </a>
                        </li>
                    </ul>
                </div>
            </div>
        </div>

        @RenderBody()
    </div>
     
</div>
 
@section Scripts
{
    <script src="~/Scripts/jquery.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="~/plugins/metismenu/js/jquery.metisMenu.js"></script>
    <script src="~/Scripts/loader.js"></script>
    @Scripts.Render("~/bundles/CreateDMPJs")
    @Scripts.Render("~/bundles/dataTables")

    <!--Knob Charts-->
    <script src="~/plugins/knob/js/jquery.knob.min.js"></script>

    <!--Jvector Map-->
    <script src="~/plugins/jvectormap/js/jquery-jvectormap-2.0.3.min.js"></script>
    <script src="~/plugins/jvectormap/js/jquery-jvectormap-world-mill-en.js"></script>

    <!--ChartJs-->
    <script src="~/plugins/chartjs/js/Chart.min.js"></script>

    <!--Morris Charts-->
    <script src="~/plugins/morris/js/raphael-min.js"></script>
    <script src="~/plugins/morris/js/morris.min.js"></script>

    <!--Float Charts-->
    <script src="~/plugins/flot/js/jquery.flot.min.js"></script>
    <script src="~/plugins/flot/js/jquery.flot.tooltip.min.js"></script>
    <script src="~/plugins/flot/js/jquery.flot.resize.min.js"></script>
    <script src="~/plugins/flot/js/jquery.flot.pie.min.js"></script>
    <script src="~/plugins/flot/js/jquery.flot.time.min.js"></script>

    <!--Functions Js-->
    <script src="~/Scripts/functions.js"></script>

    <!--Dashboard Js-->
    <script src="~/Scripts/ipdashboard.js"></script>
    <script src="~/Scripts/jsonTable.js"></script>
    <script src="~/Scripts/fns/base.js" type="text/javascript"></script>
    <script src="~/Scripts/fns/fn_dqa.js" type="text/javascript"></script>
    <script src="~/Scripts/fns/fn_dashboard.js" type="text/javascript"></script>
    <script src="~/Scripts/profile-display.js"></script>
@RenderSection("scripts", required: false)
   }