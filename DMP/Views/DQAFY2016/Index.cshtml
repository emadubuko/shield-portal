@model List<CommonUtil.Entities.AfenetReport>
    @{ 
        Layout = null;
    }
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="Cache-control" content="no-store, must-revalidate">
    <meta http-equiv="Vary" content="*">

    <title>Shield Portal</title>
    <link rel='shortcut icon' type='image/x-icon' href='~/images/umb-icon.jpg' />

    @Styles.Render("~/Content/css") 
    @Styles.Render("~/bundles/CreateDMPCss")
    <script type="text/javascript">
        {
            var profileId = '@ShieldPortal.Services.Utils.LoggedinProfileID';
        }
    </script>
    <style>
        .title {
            font-weight: 600;
            color: #000;
            padding-left: 10px;
        }
        table.dataTable tbody th, table.dataTable tbody td {
            padding: 0px 10px;
        }
        .table > tbody > tr > td, .table > tbody > tr > th, .table > tfoot > tr > td, .table > tfoot > tr > th, .table > thead > tr > td, .table > thead > tr > th {
            padding: 1px;
            line-height: 1.42857143;
            vertical-align: top;
        }
        .table > tbody > tr > td, .table > tbody > tr > th, .table > tfoot > tr > td, .table > tfoot > tr > th, .table > thead > tr > td, .table > thead > tr > th {
            padding: 1px;
            line-height: 1.42857143;
            vertical-align: middle;
        }
        .table td {
            text-align: center;
        }
        .table th {
            text-align: center; 
        }
        table.dataTable > thead > tr > th, table.dataTable th { 
            position: initial;
        }
        th.rotate {
            /* Something you can count on */
            height: 140px;
            white-space: nowrap;
        }

            th.rotate > div {
                transform: translate(25px, 51px) rotate(270deg);
                width: 10px;
                margin-right: 10px;
            }
       
    </style>
    
</head>
<body>
    <div class="page-container">

        <div class="main-header row">

            <div class="col-lg-8">
                <a class="col-lg-2">
                    <img src="~/images/shield_logo.jpg" alt="Shield Logo" width="120" title="SHIELD" />
                </a>
                <a class="col-lg-1">
                    <img src="~/images/pepfar.jpg" alt="pepfar logo" width="60" title="pepfar" />
                </a>
                <a class="col-lg-1">
                    <img src="~/images/HHS.png" alt="HHS logo" width="70" title="HHS" />
                </a>
                <a class="col-lg-2">
                    <img src="~/images/cdc.jpg" alt="cdc logo" width="100" title="cdc" />
                </a>
                <a class="col-lg-1">
                    <img src="~/images/UMB logo.jpg" alt="UMB logo" width="200" title="UMB" />
                </a>
            </div>
            <div class="col-sm-4">
                <div class="pull-right">
                    <ul class="user-info">
                        <li class="profile-info dropdown">
                            <a data-toggle="dropdown" class="dropdown-toggle" href="#" aria-expanded="false" role="button" aria-haspopup="true">
                                <img width="44" class="img-circle avatar" alt="" src="~/images/Profile-image.png"> <span id="user">@ShieldPortal.Services.Utils.DisplayBadge </span> <span class="caret"></span>
                            </a>
                            <!-- User action menu -->
                            <ul class="dropdown-menu">
                                <li><a id="userdetail" style="cursor:pointer"><i class="icon-user"></i>My profile</a></li>
                                <li><a id="changepassword" style="cursor:pointer"><i class="fa fa-key"></i>Change password</a></li>
                                <li><a href='#' id="logoff"><i class="icon-logout"></i>Logout</a></li>
                            </ul>
                            <!-- /user action menu -->
                        </li>
                    </ul>
                </div>
            </div>
            <!-- User info -->
        </div>


        <div class="main-content" style="padding-top: 0px;">
            <div class="row" id="breadcrumb">
                <div class="col-sm-5">
                    <ol class="breadcrumb breadcrumb-2">
                        <li><a href="/"><i class="icon-folder"></i>Home</a></li>
                        <li><a href="/Home/submenu?id=dmp">Data Management</a></li>
                        <li class="active">FY2016 DQA report</li>
                    </ol>
                </div>
            </div>

            <div class="row">
                @*side bar*@
                <div class="col-md-1" id="sidebar"></div>

                <div class="panel panel-success">
                    <div class="panel-heading clearfix">
                      <h3> FY 2016 DQA Report </h3> 
                    </div>
                    <div class="panel-body">
                        <div class="table-responsive" style="overflow-x: initial;">
                            <table class="table table-striped table-bordered table-hover dataTables-example" id="dataTables">
                                <thead>
                                    <tr>
                                        <th style="background-color:steelblue; color:#fff; font-weight:bold">State</th>
                                        <th style="background-color:steelblue; color:#fff; font-weight:bold">Facility</th>
                                        <th  style="background-color:steelblue; color:#fff; font-weight:bold">IP </th>
                                        <th style="background-color:steelblue; color:#fff; font-weight:bold">Service</th>
                                         
                                        <th class="rotate" style="color:black;font-weight:bold; border-left-width:medium;border-left-color:black;"><div>SAPR 16 <br /> (HTC_TST)</div></th>
                                        <th class="rotate"  style="color:black;font-weight:bold;"><div>Validated <br /> (HTC_TST)</div></th>
                                        <th class="rotate"  style="color:black;font-weight:bold;"><div>Concurrence<br /> (HTC_TST)</div></th>

                                        <th class="rotate"  style="color:black;font-weight:bold; border-left-width:medium;border-left-color:black"><div>SAPR 16 <br /> (HTC_TST_POS)</div></th>
                                        <th class="rotate"  style="color:black;font-weight:bold;"><div>Validated<br /> (HTC_TST_POS</div></th>
                                        <th class="rotate"  style="color:black;font-weight:bold;"><div>Concurrence<br /> (HTC_TST_POS)</div></th>

                                        <th class="rotate"  style="color:black;font-weight:bold; border-left-width:medium;border-left-color:black"><div>SAPR 16<br /> (PMTCT_STAT)</div></th>
                                        <th class="rotate" style="color:black;font-weight:bold;"><div>Validated<br /> (PMTCT_STAT)</div></th>
                                        <th class="rotate" style="color:black;font-weight:bold;"><div>Concurrence<br /> (PMTCT_STAT)</div></th>

                                        <th class="rotate" style="color:black;font-weight:bold; border-left-width:medium;border-left-color:black"><div>SAPR 16<br /> (PMTCT_STAT_POS)</div></th>
                                        <th class="rotate" style="color:black;font-weight:bold;"><div>Validated<br /> (PMTCT_STAT_POS)</div></th>
                                        <th class="rotate" style="color:black;font-weight:bold;"><div>Concurrence<br /> (PMTCT_STAT_POS)</div></th>

                                        <th class="rotate" style="color:black;font-weight:bold; border-left-width:medium;border-left-color:black"><div>SAPR 16<br /> (PMTCT_ARV)</div></th>
                                        <th class="rotate" style="color:black;font-weight:bold;"><div>Validated<br /> (PMTCT_ARV)</div></th>
                                        <th class="rotate" style="color:black;font-weight:bold;"><div>Concurrence<br /> (PMTCT_ARV)</div></th>

                                        <th class="rotate" style="color:black;font-weight:bold; border-left-width:medium;border-left-color:black"><div>SAPR 16<br /> (TX_NEW)</div></th>
                                        <th class="rotate" style="color:black;font-weight:bold;"><div>Validated<br /> (TX_NEW)</div></th>
                                        <th class="rotate" style="color:black;font-weight:bold;"><div>Concurrence<br /> (TX_NEW)</div></th>

                                        <th class="rotate" style="color:black;font-weight:bold; border-left-width:medium;border-left-color:black"><div>SAPR 16<br /> (TX_CURR)</div></th>
                                        <th class="rotate" style="color:black;font-weight:bold;"><div>Validated<br /> (TX_CURR)</div></th>
                                        <th class="rotate" style="color:black;font-weight:bold;"><div>Concurrence<br /> (TX_CURR)</div></th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @foreach (var item in Model)
                                    {
                                        <tr>
                                            <td>@item.State</td>
                                            <td>@item.Facility</td>
                                            <td>@item.IP</td>
                                            <td>@item.Services</td>

                                            <td style="border-left-width:medium;border-left-color:black">@item.HTC_TST.SAPR16</td>
                                            <td>@item.HTC_TST.Validated</td>
                                            <td style=@(item.HTC_TST.Concurrence > 89 && item.HTC_TST.Concurrence < 110 ? "background-color:yellowgreen;color:#fff" : "background-color:sandybrown;color:#fff")>@item.HTC_TST.Concurrence.ToString().Split('.')[0] %</td>

                                            <td style="border-left-width:medium;border-left-color:black">@item.HTC_TST_POS.SAPR16</td>
                                            <td>@item.HTC_TST_POS.Validated</td>
                                            <td style=@(item.HTC_TST_POS.Concurrence > 89 && item.HTC_TST_POS.Concurrence < 110 ? "background-color:yellowgreen;color:#fff" : "background-color:sandybrown;color:#fff")>@item.HTC_TST_POS.Concurrence.ToString().Split('.')[0] %</td>

                                            <td style="border-left-width:medium;border-left-color:black">@item.PMTCT_STAT.SAPR16</td>
                                            <td>@item.PMTCT_STAT.Validated</td>
                                            <td style=@(item.PMTCT_STAT.Concurrence > 89 && item.PMTCT_STAT.Concurrence < 110 ? "background-color:yellowgreen;color:#fff" : "background-color:sandybrown;color:#fff")>@item.PMTCT_STAT.Concurrence.ToString().Split('.')[0] %</td>

                                            <td style="border-left-width:medium;border-left-color:black">@item.PMTCT_STAT_POS.SAPR16</td>
                                            <td>@item.PMTCT_STAT_POS.Validated</td>
                                            <td style=@(item.PMTCT_STAT_POS.Concurrence > 89 && item.PMTCT_STAT_POS.Concurrence < 110 ? "background-color:yellowgreen;color:#fff" : "background-color:sandybrown;color:#fff")>@item.PMTCT_STAT_POS.Concurrence.ToString().Split('.')[0] %</td>

                                            <td style="border-left-width:medium;border-left-color:black">@item.PMTCT_ARV.SAPR16</td>
                                            <td>@item.PMTCT_ARV.Validated</td>
                                            <td style=@(item.PMTCT_ARV.Concurrence > 89 && item.PMTCT_ARV.Concurrence < 110 ? "background-color:yellowgreen;color:#fff" : "background-color:sandybrown;color:#fff")>@item.PMTCT_ARV.Concurrence.ToString().Split('.')[0] %</td>

                                            <td style="border-left-width:medium;border-left-color:black">@item.TX_NEW.SAPR16</td>
                                            <td>@item.TX_NEW.Validated</td>
                                            <td style=@(item.TX_NEW.Concurrence > 89 && item.TX_NEW.Concurrence < 110 ? "background-color:yellowgreen;color:#fff" : "background-color:sandybrown;color:#fff")>@item.TX_NEW.Concurrence.ToString().Split('.')[0] %</td>

                                            <td style="border-left-width:medium;border-left-color:black">@item.TX_CURR.SAPR16</td>
                                            <td>@item.TX_CURR.Validated</td>
                                            <td style=@(item.TX_CURR.Concurrence > 89 && item.TX_CURR.Concurrence < 110 ? "background-color:yellowgreen;color:#fff" : "background-color:sandybrown;color:#fff")>@item.TX_CURR.Concurrence.ToString().Split('.')[0] %</td>

                                        </tr>
                                    }
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>


                 
            </div>

            <div class="row">
                <div class="col-md-push-3 col-md-5">
                    <footer class="footer-main">
                        &copy; @DateTime.Now.Year  <span>University of Maryland</span>
                    </footer>
                </div>
            </div>

        </div>
    </div>

    <script src="~/Scripts/jquery.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="~/plugins/metismenu/js/jquery.metisMenu.js"></script>
    <script src="~/Scripts/loader.js"></script>
    @Scripts.Render("~/bundles/CreateDMPJs")
    @Scripts.Render("~/bundles/dataTables")
     
    <script src="~/Scripts/profile-display.js"></script> 
</body>
</html>